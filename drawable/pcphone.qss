	background-color:rgba(68,68,68,1);
}

AudioProgress #btnPlay{
	background:transparent;
	qproperty-icon:url(":/SilkVoiceDemo/Resources/play.png");
}

AudioProgress #labDur{
	color:rgba(255,255,255,1);
}

AudioProgress #labTotalDur{
	color:rgba(255,255,255,1);
}

AudioProgress #line
{
	background-color:rgba(233,233,233,65.24%)
}

MicTest #contentWidget{
	border-radius:8px;
	background-color:rgba(255,255,255,1);
